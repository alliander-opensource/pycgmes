  <rdf:Description rdf:about="#DiagramObject.IdentifiedObject-DLvalueTypeSparql">
    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#SPARQLConstraint"/>
    <sh:message>One of the following does not conform: 1) The value type shall be IRI; 2) The value type shall not be an instance of the class: cim:Diagram, cim:DiagramObject, cim:VisibilityLayer, cim:DiagramStyle, cim:DiagramObjectStyle or cim:TextDiagramObject.</sh:message>
    <sh:prefixes rdf:resource="http://iec.ch/TC57/CIM100#"/>
    <sh:select>
	      SELECT $this  ?value
        WHERE {
          ?value cim:DiagramObject.IdentifiedObject/rdf:type ?valueType .  
          {
            SELECT $this ?value
            WHERE {
              ?value rdf:type ?classtype .
              FILTER (?classtype IN (cim:TextDiagramObject , cim:DiagramObject)).
              }
          }
          FILTER (?valueType IN (cim:Diagram , cim:DiagramObject, cim:VisibilityLayer , cim:DiagramStyle , cim:DiagramObjectStyle , cim:TextDiagramObject)).
        } 
        </sh:select>
  </rdf:Description>
  <rdf:Description rdf:about="#DiagramObject.IdentifiedObject-DLvalueType">
    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#PropertyShape"/>
    <sh:sparql rdf:resource="#DiagramObject.IdentifiedObject-DLvalueTypeSparql"/>
    <sh:path rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
    <sh:description>The association end role description stated that: The domain object to which this diagram object is associated. Therefore, the association cannot point to cim:Diagram, cim:DiagramObject, cim:VisibilityLayer, cim:DiagramStyle, cim:DiagramObjectStyle or cim:TextDiagramObject.</sh:description>
    <sh:group rdf:resource="#DL301UML"/>
    <sh:name>C:301:DL:DiagramObject.IdentifiedObject:internalValueType</sh:name>
    <sh:nodeKind rdf:resource="http://www.w3.org/ns/shacl#IRI"/>
    <sh:order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:order>
    <sh:severity rdf:resource="http://www.w3.org/ns/shacl#Violation"/>
  </rdf:Description>
