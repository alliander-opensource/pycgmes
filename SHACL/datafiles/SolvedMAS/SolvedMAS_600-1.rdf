    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#PropertyGroup"/>
    <rdfs:label>SolvedMAS600</rdfs:label>
    <sh:order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</sh:order>
  </rdf:Description>
