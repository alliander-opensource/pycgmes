    <sh:property rdf:resource="#RegulatingControl-requiredAttributes"/>
    <sh:targetClass rdf:resource="http://iec.ch/TC57/CIM100#RegulatingControl"/>
    <sh:targetClass rdf:resource="http://iec.ch/TC57/CIM100#TapChangerControl"/>
