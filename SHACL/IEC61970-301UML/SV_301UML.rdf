  <rdf:Description rdf:about="#CsConverter.gamma-valueRangeTypicalSparql">
    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#SPARQLConstraint"/>
    <sh:message>The value is outside the range: 17-20 degrees.</sh:message>
    <sh:prefixes rdf:resource="http://iec.ch/TC57/CIM100#"/>
    <sh:select>
			SELECT  $this ?value
			WHERE {
        $this cim:CsConverter.operatingMode cim:CsOperatingModeKind.inverter .
        $this $PATH ?value .
        FILTER (?value&lt;17 || ?value&gt;20) .        
			}</sh:select>
  </rdf:Description>
