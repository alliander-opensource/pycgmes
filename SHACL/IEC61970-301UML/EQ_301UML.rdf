    <sh:select>
			SELECT  $this ?value
			WHERE {
        $this $PATH ?value .
        BIND(EXISTS{$this $PATH ?v} AS ?hasvalue).
        #OPTIONAL {$this $PATH ?value }. 
        $this ^cim:RotatingMachine.GeneratingUnit/cim:RotatingMachine.ratedS ?rateds .
