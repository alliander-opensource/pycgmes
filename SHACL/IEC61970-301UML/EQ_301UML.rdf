    <sh:prefixes rdf:resource="http://iec.ch/TC57/CIM100#"/>
    <sh:select>
			SELECT  $this ?value
			WHERE {
        ?value cim:PowerTransformerEnd.PowerTransformer $this .  
        ?value cim:TransformerEnd.endNumber 2 .
        #?value cim:TransformerEnd.endNumber ?endnumber .
