  <rdf:Description rdf:about="#ControlAreaGeneratingUnit.GeneratingUnit-instanceSparql">
    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#SPARQLConstraint"/>
    <sh:message>The GeneratingUnit is assigned to more than once in a ControlArea.</sh:message>
    <sh:prefixes rdf:resource="http://iec.ch/TC57/CIM100#"/>
    <sh:select>
			SELECT  $this (COUNT(?ca) AS ?count)
			WHERE {
        $this ^cim:ControlAreaGeneratingUnit.GeneratingUnit/cim:ControlAreaGeneratingUnit.ControlArea ?ca.   
			}
      GROUP BY $this ?ca
      HAVING(?count&gt;1)
      </sh:select>
  </rdf:Description>
