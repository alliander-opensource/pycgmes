  <rdf:Description rdf:about="#EquivalentBranch.x21-usageSparql">
    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#SPARQLConstraint"/>
    <sh:message>Asymmetrical EquivalentBranch is modelled as EquivalentBranch.x is different from EquivalentBranch.x21.</sh:message>
    <sh:prefixes rdf:resource="http://iec.ch/TC57/CIM100#"/>
    <sh:select>
			SELECT  $this ?value
			WHERE {
        OPTIONAL {$this $PATH ?value }.
        $this cim:EquivalentBranch.x ?x .
        FILTER (bound(?value) &amp;&amp; ?value!=?x) .
			}</sh:select>
  </rdf:Description>
