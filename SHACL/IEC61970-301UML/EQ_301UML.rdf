        #OPTIONAL {?machine cim:RotatingMachine.ratedS ?rateds }. 
        FILTER (?hasvalue=true &amp;&amp; ?hasrateds=true &amp;&amp; (?value&lt;=0 || ?value&gt;?rateds) ) .        
			}</sh:select>
