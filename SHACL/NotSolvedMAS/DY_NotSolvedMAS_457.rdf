    <sh:targetClass rdf:resource="http://iec.ch/TC57/CIM100#GovHydroPID"/>
    <sh:targetClass rdf:resource="http://iec.ch/TC57/CIM100#GovSteamFV2"/>
    <sh:targetClass rdf:resource="http://iec.ch/TC57/CIM100#GovSteamEU"/>
