  <rdf:Description rdf:about="#HydroGeneratingUnit.energyConversionCapability-typeConsistency">
    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#PropertyShape"/>
    <sh:sparql rdf:resource="#HydroGeneratingUnit.energyConversionCapability-typeConsistencySparql"/>
