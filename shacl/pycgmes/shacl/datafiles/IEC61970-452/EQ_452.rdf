    <sh:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:minCount>
    <sh:severity rdf:resource="http://www.w3.org/ns/shacl#Violation"/>
    <sh:message>The containment is either missing or it is not Substation.</sh:message>
    <sh:order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</sh:order>
