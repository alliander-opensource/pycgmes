  <rdf:Description rdf:about="#ACLineSegment.x-valueRange">
    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#PropertyShape"/>
    <sh:description>The attribute shall be a positive value. As negative reactance values are not allowed for ACLineSegment-s it is recommended to model series compensators explicitly.</sh:description>
    <sh:group rdf:resource="#EQ452"/>
    <sh:message>The value is negative or zero.</sh:message>
    <sh:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.0</sh:minExclusive>
    <sh:name>C:452:EQ:ACLineSegment.x:valueRange</sh:name>
    <sh:order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</sh:order>
